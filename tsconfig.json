{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "types": ["node"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noEmit": true,
    "outDir": "dist"
  }
}
